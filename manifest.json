{
    "manifest_version": 3,
    "name": "Data Tracker Visualizer",
    "version": "1.0",
    "description": "Visualize and track third-party trackers in real-time.",
    "permissions": ["webRequest", "activeTab", "storage", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  